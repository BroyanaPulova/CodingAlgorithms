
@{
    ViewBag.Title = "Example";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="jumbotron home home-fullscreen" id="home">
    <div class="mask"></div>
    <a href="#" class="logo">
        <img src="~/Content/IMG/96x96.png" alt="Moderno - Theme by GraphBerry.com">
    </a>
    <!-- Offsite navigation -->
    <div class="container">
        <div class="header-info">
            <h1>ШУМОЗАЩИТНО КОДИРАНЕ</h1>
            <p>
                кодиране на информацията с цел с запазването и при предаването в каналите за връзка.
            </p>
            <a href="/Coding/Coding" class="btn btn-primary">ОЩЕ>></a>
        </div>
    </div>
</div>
<!-- Services section end -->
<!-- Portfolio section start -->
<section id="portfolio" class="darker">
    <div class="container">
        <header>
            <h2>ПРИЛОЖЕНИЕ</h2>
        </header>
    </div>
    <div id="single-project"></div>
    <div class="row">
        <div class="container">
            <div class="col-md-6 text-xs-right">
                <div class="panel panel-info">
                    <div class="panel-heading">Входни данни</div>
                    <div class="panel-body">
                        <div class="row">
                            <div class="container">
                                <label for="comment">Дължина на кода:</label>
                                <input type="text" class="form-control" style="width:44.7%" id="length">
                                <label for="comment">Двоичен вектор в десетичен вид:</label>
                                <input type="text" class="form-control" style="width:44.7%" id="vector">
                                <br />
                                <button type="submit" class="btn btn-primary">Напред</button>
                            </div>
                        </div>
                        <br />
                        <div class="form-group">
                            <label for="comment">Въведете Y:</label>
                            <input type="text" class="form-control" id="yInput">
                            <br />
                            <input  onclick="meggitDecode()" type="submit" class="btn btn-primary" value="Напред" />
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 text-xs-right">
                <div class="panel panel-info">
                    <div class="panel-heading">Резултат</div>
                    <div class="panel-body">
                        <div class="form-group">
                            <label for="comment">Правилният Y:</label>
                            <input type="text" readonly class="form-control" id="Youtput">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- Contact section end -->
<!-- Footer start -->

